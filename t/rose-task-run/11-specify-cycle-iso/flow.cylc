#!jinja2
[cylc]
    UTC mode=True
    [[events]]
        abort on timeout=True
        timeout=PT1M
[scheduling]
    initial cycle point=20130101T0000Z
    final cycle point=20130101T0000Z
    [[dependencies]]
        [[[T00]]]
            graph=t1

[runtime]
    [[t1]]
        script=rose task-run --cycle=PT12H
        [[[job]]]
           execution time limit=PT1M
