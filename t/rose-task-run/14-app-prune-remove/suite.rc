#!jinja2
[cylc]
    UTC mode = True
    abort if any task fails = True
    [[events]]
        abort on timeout = True
        timeout = PT1M
[scheduling]
    initial cycle point = 20100101
    final cycle point = 20100105
    [[dependencies]]
        [[[T00]]]
            graph = prune[-P1D] => my_task_1 => prune

[runtime]
    [[root]]
        script = rose task-run -v -v --debug
        [[[job]]]
           execution time limit = PT1M
    [[my_task_1]]
    [[prune]]
