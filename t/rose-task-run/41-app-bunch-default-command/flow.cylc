#!jinja2
[cylc]
    UTC mode=True
    [[events]]
        abort on timeout=True
        timeout=PT1M
[scheduling]
    initial cycle point=20100101
    final cycle point=20100101
    [[dependencies]]
        [[[T00]]]
            graph = buncher_default & buncher_import
[runtime]
    [[buncher_default, buncher_import]]
        script=rose task-run -v -v --debug
        [[[job]]]
            execution time limit=PT1M

